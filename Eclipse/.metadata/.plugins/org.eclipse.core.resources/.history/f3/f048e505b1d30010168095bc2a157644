package edu.pipeline;

import java.util.List;

import edu.data.Stmt;

public class StatementData {

	public final List<Stmt> statements;
	public final List<edu.error.IError> errors;
	public final Rootpath rootpath;
	
	public StatementData(List<Stmt> statements, List<edu.error.IError> errors, Rootpath rootpath) {
		this.statements = statements;
		this.errors = errors;
		this.rootpath = rootpath;
	}
	public StatementData(List<Stmt> statements, List<edu.error.IError> errors) {
		this(statements, errors, null);
	}
}
